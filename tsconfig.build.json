{
  "compilerOptions": {
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "target": "esnext",
    "sourceMap": true,
    "strict": true,
    "declaration": true,
    "declarationMap": true,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "types": [],
    "lib": ["esnext", "dom", "dom.iterable"],
    "noEmit": false
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
