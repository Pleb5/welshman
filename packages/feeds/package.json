  "devDependencies": {
    "gts": "^5.0.1",
    "tsc-multi": "^1.1.0",
    "typescript": "~5.1.6"
  },
  "dependencies": {
    "@welshman/lib": "~0.0.23",
    "@welshman/util": "~0.0.42"
  }
