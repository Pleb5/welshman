{
  "name": "@welshman/app",
  "version": "0.0.31",
  "author": "hodlbod",
  "license": "MIT",
  "description": "A collection of svelte stores for use in building nostr client applications.",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "build"
  ],
  "types": "./build/src/index.d.ts",
